{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb", "plugin:import/errors", "plugin:import/warnings"],
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "."]
        }
      }
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "import/prefer-default-export": 1,
      "react/jsx-props-no-spreading": 1
    }
}