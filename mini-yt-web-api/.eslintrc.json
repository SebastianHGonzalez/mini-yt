{
    "env": {
      "jest": true,
      "node": true,
  "es6": true
    },
    "extends": ["airbnb-base", "plugin:import/errors", "plugin:import/warnings", "plugin:jest/recommended"],
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "./src"]
        }
      }
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
      "import/prefer-default-export": 1
    }
}